    "authors": [
      "Dhyces"
    ],
    "contact": {
      "homepage": "https://www.curseforge.com/minecraft/mc-mods/charm-of-return",
      "sources": "https://github.com/dhyces/ring-of-return",
      "issues": "https://github.com/dhyces/ring-of-return/issues"
    },
  
